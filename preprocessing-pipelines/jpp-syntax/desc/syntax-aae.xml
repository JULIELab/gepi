<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>  
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="jcore-jsbd-biomedical-english">
      <import name="de.julielab.jcore.ae.jsbd.desc.jcore-jsbd-ae-biomedical-english"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="jcore-acronym-ae">
      <import name="de.julielab.jcore.ae.acronymtagger.desc.jcore-acronym-ae"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="jcore-jtbd-biomedical-english">
      <import name="de.julielab.jcore.ae.jtbd.desc.jcore-jtbd-ae-biomedical-english"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="jcore-opennlp-postag-ae-biomedical-english">
      <import name="de.julielab.jcore.ae.opennlp.postag.desc.jcore-opennlp-postag-ae-biomedical-english"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="jcore-stanford-lemmatizer">
      <import name="de.julielab.jcore.ae.stanford.lemma.desc.jcore-stanford-lemmatizer"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>syntax-aae</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>AcroList</name>
        <description>A list of acronym-fullForm pairs known not to be recognized by the JACRO algorithm:
Format of the file

acronym1 TAB full form1
acronym2 TAB full form2
etc.</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>jcore-acronym-ae/AcroList</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>AcroList</name>
        <value>
          <string>resources/acrolist.txt</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>jcore-jsbd-biomedical-english</node>
        <node>jcore-acronym-ae</node>
        <node>jcore-jtbd-biomedical-english</node>
        <node>jcore-opennlp-postag-ae-biomedical-english</node>
        <node>jcore-stanford-lemmatizer</node>
      </fixedFlow>
    </flowConstraints>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type>de.julielab.jcore.types.Token</type>
          <type allAnnotatorFeatures="true">de.julielab.jcore.types.Sentence</type>
          <type allAnnotatorFeatures="true">de.julielab.jcore.types.Abbreviation</type>
          <feature>de.julielab.jcore.types.Token:posTag</feature>
          <feature>de.julielab.jcore.types.Token:lemma</feature>
          <feature>de.julielab.jcore.types.Token:componentId</feature>
        </outputs>
        <languagesSupported/>
      </capability>
    </capabilities>
  <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
