<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="ChunkerGENIA">
      <import location="ChunkerGENIA.xml"/>
    </delegateAnalysisEngine>
<!--     <delegateAnalysisEngine key="MSTParserGENIA"> -->
<!--       <import location="MSTParserGENIA.xml"/> -->
<!--     </delegateAnalysisEngine> -->
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>SyntaxAnnotator</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>chunkerModelFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>ChunkerGENIA/modelFile</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>chunkerPosTagSetPref</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>ChunkerGENIA/posTagSetPref</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>chunkerMappings</name>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>ChunkerGENIA/mappings</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>chunkerModelFile</name>
        <value>
          <string>/var/data/essentials/pipeline_data/models/Chunker_Genia.bin.gz</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>chunkerMappings</name>
        <value>
          <array>
            <string>NP;de.julielab.jules.types.ChunkNP</string>
            <string>PP;de.julielab.jules.types.ChunkPP</string>
            <string>VP;de.julielab.jules.types.ChunkVP</string>
            <string>ADJP;de.julielab.jules.types.ChunkADJP</string>
            <string>CONJP;de.julielab.jules.types.ChunkCONJP</string>
            <string>LST;de.julielab.jules.types.ChunkLST</string>
            <string>SBAR;de.julielab.jules.types.ChunkSBAR</string>
            <string>PRT;de.julielab.jules.types.ChunkPRT</string>
            <string>ADVP;de.julielab.jules.types.ChunkADVP</string>
          </array>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
<!--         <node>MSTParserGENIA</node> -->
        <node>ChunkerGENIA</node>
      </fixedFlow>
    </flowConstraints>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
