<?xml version="1.0" encoding="UTF-8"?>

<cpeDescription xmlns="http://uima.apache.org/resourceSpecifier">
	<!--Relations (ROGE and Binding Relations!) Apply this CPE for extraction 
		of gene regulation relations, fro example for Candida, Fumigatus Pilze or 
		for regulonDB documents! -->
	<collectionReader>
		<collectionIterator>
			<descriptor>
				<import location="../../desc/jules-cas-xmi-from-db-reader.xml" />
			</descriptor>
			<configurationParameterSettings>
				<nameValuePair>
					<name>Table</name>
					<value>
						<string>_2relations_mirror3</string>
<!-- <string>semedico.testdoc</string> -->
					</value>
				</nameValuePair>
								<nameValuePair>
					<name>ResetTable</name>
					<value>
						<boolean>false</boolean>
					</value>
				</nameValuePair>
				<nameValuePair>
					<name>AdditionalTables</name>
					<value>
						<array>
							<string>de.julielab.jcore.types.Sentence</string>
							<string>de.julielab.jcore.types.Token</string>
<!-- 							<string>de.julielab.jcore.types.PennBioIEPOSTag</string> -->
							<string>de.julielab.jcore.types.Abbreviation</string>
						</array>
					</value>
				</nameValuePair>
				<nameValuePair>
					<name>BatchSize</name>
					<value>
						<integer>100</integer>
					</value>
				</nameValuePair>

			</configurationParameterSettings>
		</collectionIterator>
	</collectionReader>
	<casProcessors casPoolSize="32"
		processingUnitThreadCount="1">

		<casProcessor deployment="integrated" name="EntityAAE">
			<descriptor>
				<import location="EntityAAE.xml" />
			</descriptor>
			<deploymentParameters />
			<errorHandling>
				<errorRateThreshold action="terminate" value="10/1000" />
				<maxConsecutiveRestarts action="terminate"
					value="30" />
				<timeout max="100000" default="-1" />
			</errorHandling>
			<checkpoint batch="1" time="1000ms" />
		</casProcessor>
		<!-- <casProcessor deployment="integrated" name="MeSHGazetteer"> -->
		<!-- <descriptor> -->
		<!-- <import location="MeshGazetteerAnnotator.xml" /> -->
		<!-- </descriptor> -->
		<!-- <deploymentParameters /> -->
		<!-- <errorHandling> -->
		<!-- <errorRateThreshold action="terminate" value="10/1000" /> -->
		<!-- <maxConsecutiveRestarts action="terminate" -->
		<!-- value="30" /> -->
		<!-- <timeout max="100000" default="-1" /> -->
		<!-- </errorHandling> -->
		<!-- <checkpoint batch="1" time="1000ms" /> -->
		<!-- </casProcessor> -->

		<casProcessor deployment="integrated" name="BioPortalSimpleGazetteer">
			<descriptor>
				<import location="BioPortalSimpleGazetteerAnnotator.xml" />
			</descriptor>
			<deploymentParameters />
			<errorHandling>
				<errorRateThreshold action="terminate" value="10/1000" />
				<maxConsecutiveRestarts action="terminate"
					value="30" />
				<timeout max="100000" default="-1" />
			</errorHandling>
			<checkpoint batch="1" time="1000ms" />
		</casProcessor>

		<casProcessor deployment="integrated" name="BioPortalAggregatesGazetteer">
			<descriptor>
				<import location="BioPortalAggregatesGazetteerAnnotator.xml" />
			</descriptor>
			<deploymentParameters />
			<errorHandling>
				<errorRateThreshold action="terminate" value="10/1000" />
				<maxConsecutiveRestarts action="terminate"
					value="30" />
				<timeout max="100000" default="-1" />
			</errorHandling>
			<checkpoint batch="1" time="1000ms" />
		</casProcessor>

		<casProcessor deployment="integrated" name="BioPortalAggElementsGazetteer">
			<descriptor>
				<import location="BioPortalAggElementsGazetteerAnnotator.xml" />
			</descriptor>
			<deploymentParameters />
			<errorHandling>
				<errorRateThreshold action="terminate" value="10/1000" />
				<maxConsecutiveRestarts action="terminate"
					value="30" />
				<timeout max="100000" default="-1" />
			</errorHandling>
			<checkpoint batch="1" time="1000ms" />
		</casProcessor>

		<casProcessor deployment="integrated" name="CasToXmiDBConsumer">
			<descriptor>
				<import location="../../desc/jules-cas-xmi-to-db-consumer.xml" />
			</descriptor>
			<deploymentParameters />
			<errorHandling>
				<errorRateThreshold action="terminate" value="0/1000" />
				<maxConsecutiveRestarts action="terminate"
					value="30" />
				<timeout max="100000" default="-1" />
			</errorHandling>
			<checkpoint batch="100" time="1000ms" />
			<configurationParameterSettings>
				<!-- An override must be defined in the CPE descriptor because otherwise 
					we won't be ably to change the value from Java code (at least I currently 
					don't know how). -->
				<nameValuePair>
					<name>UpdateMode</name>
					<value>
						<boolean>false</boolean>
					</value>
				</nameValuePair>
				<nameValuePair>
					<name>AnnotationsToStore</name>
					<value>
						<array>
							<!-- Comes with BioPortal now, can probably be deleted. -->
							<!-- <string>de.julielab.jules.types.MeshMention</string> -->
							<string>de.julielab.jcore.types.Cell</string>
							<string>de.julielab.jcore.types.stemnet.CD_antigens</string>
							<string>de.julielab.jcore.types.stemnet.MinorHA</string>
							<string>de.julielab.jcore.types.OntClassMentionSimple</string>
							<string>de.julielab.jcore.types.OntClassMentionAggregate</string>
							<string>de.julielab.jcore.types.OntClassMentionAggElement
							</string>
							<string>de.julielab.jcore.types.AutoDescriptor</string>
<!-- 							<string>de.julielab.jcore.types.DocumentClass</string> -->
						</array>
					</value>
				</nameValuePair>
			</configurationParameterSettings>
		</casProcessor>
	</casProcessors>
	<cpeConfig>
		<numToProcess>-1</numToProcess>
		<deployAs>immediate</deployAs>
		<checkpoint batch="1000" />
		<timerImpl />
	</cpeConfig>
</cpeDescription>
