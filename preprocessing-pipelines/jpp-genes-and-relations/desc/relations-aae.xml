<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>  
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="jcore-biosem-ae-bionlp-st11">
      <import name="de.julielab.jcore.ae.biosem.desc.jcore-biosem-ae-bionlp-st11"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="jules-likelihood-detection-ae">
      <import name="jules-likelihood-detection-ae"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="jules-likelihood-assignment-ae">
      <import name="jules-likelihood-assignment-ae"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>relations-aae</name>
    <description>WARNING: GeNo is NOT included here because it is installed on a server and the UIMA AAE editor will continue to complain that it cannot find the AAE locally.</description>
    <version>1.0</version>
    <vendor/>
    <configurationParameters>
      <configurationParameter>
        <name>likelihoodDict</name>
        <description>Path to likelihood dictionary file.</description>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>jules-likelihood-detection-ae/likelihoodDict</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>likelihoodDict</name>
        <value>
          <string>resources/likelihood_neg_invest_dict</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>jcore-biosem-ae-bionlp-st11</node>
        <node>jules-likelihood-detection-ae</node>
        <node>jules-likelihood-assignment-ae</node>
      </fixedFlow>
    </flowConstraints>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported/>
      </capability>
    </capabilities>
  <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
