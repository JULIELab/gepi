		<casProcessor deployment="integrated" name="EventFlattener">
			<descriptor>
				<import location="AEDescriptors/jules-event-flattener-ae.xml" />
			</descriptor>
			<deploymentParameters />
			<errorHandling>
				<errorRateThreshold action="terminate" value="10/1000" />
				<maxConsecutiveRestarts action="terminate"
					value="30" />
				<timeout max="100000" default="-1" />
			</errorHandling>
			<checkpoint batch="1" time="1000ms" />
		</casProcessor>
		<casProcessor deployment="integrated" name="AnnotationSplitterAnnotator">
			<descriptor>
				<import location="AEDescriptors/AnnotationSplitterAnnotator.xml" />
			</descriptor>
			<deploymentParameters />
			<errorHandling>
				<errorRateThreshold action="terminate" value="10/1000" />
				<maxConsecutiveRestarts action="terminate"
					value="30" />
				<timeout max="100000" default="-1" />
			</errorHandling>
			<checkpoint batch="1" time="1000ms" />
			<configurationParameterSettings>
				<nameValuePair>
					<name>splitterDescriptionFileName</name>
					<value>
						<string>resources/LucasParams/splitter.xml</string>
					</value>
				</nameValuePair>
			</configurationParameterSettings>
		</casProcessor>
		<casProcessor deployment="integrated" name="TermExtractor">
			<descriptor>
				<import location="AEDescriptors/jules-concept-term-extractor.xml" />
			</descriptor>
			<configurationParameterSettings>
				<nameValuePair>
					<name>Types</name>
					<value>
						<array>
							<string>de.julielab.jules.types.Gene=/resourceEntryList/entryId
							</string>
							<string>de.julielab.jules.types.EventMention=/specificType
							</string>
							<string>de.julielab.jules.types.Cell=/specificType</string>
							<string>de.julielab.jules.types.stemnet.CD_antigens=/specificType
							</string>
							<string>de.julielab.jules.types.stemnet.MinorHA=/specificType
							</string>
							<string>de.julielab.jules.types.OntClassMentionSimple=/specificType
							</string>
							<string>de.julielab.jules.types.OntClassMentionAggElement=/specificType
							</string>
						</array>
					</value>
				</nameValuePair>
				<nameValuePair>
					<name>Neo4jAddress</name>
					<value>
						<string>http://localhost:7474</string>
					</value>
				</nameValuePair>
			</configurationParameterSettings>
			<deploymentParameters />
			<errorHandling>
				<errorRateThreshold action="terminate" value="10/1000" />
				<maxConsecutiveRestarts action="terminate"
					value="30" />
				<timeout max="100000" default="-1" />
			</errorHandling>
			<checkpoint batch="500" time="1000ms" />
		</casProcessor>
