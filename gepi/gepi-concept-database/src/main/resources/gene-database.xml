<?xml version="1.0"?>
<conceptdatabase xmlns="http://www.julielab.de/conceptdatabase"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.julielab.de/conceptdatabase conceptdatabase.xsd">
	<versioning>
		<version>1.0</version>
	</versioning>
	<connection>
		<uri>http://localhost:7474</uri>
		<user>neo4j</user>
		<password>erik</password>
	</connection>
	<imports>
		<import>
			<conceptcreator>de.julielab.concepts.db.creators.NCBIGeneConceptCreator</conceptcreator>
			<configuration>
				<pluginname>ConceptManager</pluginname>
				<pluginendpoint>insert_concepts</pluginendpoint>
				<basepath>src/test/resources/geneconcepts/</basepath>
				<gene_info>genes/gene_info_test</gene_info>
				<genedescriptions>genes/gene2summary_test</genedescriptions>
				<organismlist>organisms/taxIdsForTests.lst</organismlist>
				<organismnames>organisms/names_test.dmp</organismnames>
				<homologene>genes/homologene_test.data</homologene>
				<gene_group>genes/gene_group_test</gene_group>
			</configuration>
		</import>
	</imports>
	<exports>
		<export>
			<exporter>ServerPluginExporter</exporter>
			<configuration>
				<pluginname>
					Export
				</pluginname>
				<pluginendpoint>
					term_id_mapping
				</pluginendpoint>
				<outputfile>
					genes.idmap
				</outputfile>
				<decoding>
					<json2bytearray>true</json2bytearray>
					<base64>false</base64>
					<gzip>true</gzip>
				</decoding>
				<parameters>
					<id_property>
						originalId
					</id_property>
					<labels tojson="true">
						<label>ID_MAP_NCBI_GENES</label>
					</labels>
				</parameters>
			</configuration>
		</export>
		<export>
			<exporter>ServerPluginExporter</exporter>
			<configuration>
				<pluginname>
					Export
				</pluginname>
				<pluginendpoint>
					element_aggregate_id_mapping
				</pluginendpoint>
				<outputfile>
					elements.2aggid
				</outputfile>
				<decoding>
					<json2bytearray>false</json2bytearray>
					<base64>true</base64>
					<gzip>true</gzip>
				</decoding>
				<parameters>
					<labels tojson="true">
						<label>AGGREGATE_TOP_HOMOLOGY</label>
						<label>AGGREGATE_HOMOLOGENE</label>
						<label>AGGREGATE_GENEGROUP</label>
					</labels>
				</parameters>
			</configuration>
		</export>
		<export>
			<exporter>ServerPluginExporter</exporter>
			<configuration>
				<pluginname>
					Export
				</pluginname>
				<pluginendpoint>
					hypernyms
				</pluginendpoint>
				<outputfile>
					hypernyms.tid
				</outputfile>
				<decoding>
					<json2bytearray>true</json2bytearray>
					<base64>false</base64>
					<gzip>true</gzip>
				</decoding>
				<parameters/>
			</configuration>
		</export>
	</exports>
</conceptdatabase>
