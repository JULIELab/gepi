<!DOCTYPE html>
<div xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd" xmlns:p="tapestry:parameter" t:type="gepiwidgetlayout"
     widgettitle="Circle Widget" t:clientId="${elementId}-outer" class="${classes}"
     sizeClass="small" esResult="esResult" neo4jResult="neo4jResult" useTapestryZoneUpdates="false"
     dataSessionId="dataSessionId">

    <div class="row full-height">
        <div class="${chartAreaColumnSizeClass} text-center full-height">
            <div id="${elementId}" class="chart-area full-height"/>
        </div>
        <div class="col-2">
            <div id="${elementId}-container">
                <div class="settings">
                    <div class="checkboxes"></div>
                    <div class="sliders"></div>
                </div>
            </div>
        </div>
    </div>
</div>