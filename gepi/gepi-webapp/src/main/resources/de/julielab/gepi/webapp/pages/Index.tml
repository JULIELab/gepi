<html t:type="layout" title="GePI Index"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd">
    <body class="${bodyScrollClass}">
        <div class="row">
            <div id="inputcol" class="col-md-4 col-md-offset-4 fade ${showInputClass}">
                <t:zone t:id="inputzone" id="inputzone">
                    <t:gepiinput result="result"/>
                </t:zone>
            </div>
        </div>
        <div id="disableplane" class="fade"/>
        <div class="row">
            <div id="outputcol" class="col-md-12 fade ${showOutputclass}">
                <t:zone t:id="outputzone" id="outputzone">
                    <div class="grid-stack data-gs-animate">
                        <div id="widgetOverlay" class="fade ${widgetOverlayShowClass}"/>
                        <div class="grid-stack-item" data-gs-x="0" data-gs-y="0" data-gs-width="3" data-gs-height="4">
                            <div class="grid-stack-item-content">
                                <t:piechartwidget result="result"/>
                            </div>
                        </div>
                        <div class="grid-stack-item" data-gs-x="3" data-gs-y="0" data-gs-width="3" data-gs-height="4">
                            <div class="grid-stack-item-content">
                                <t:sankeywidget result="result" elementId="sankeychart" commonPartners="false"/>
                            </div>
                        </div>
                        <div class="grid-stack-item" data-gs-x="6" data-gs-y="0" data-gs-width="3" data-gs-height="4">
                            <div class="grid-stack-item-content">
                                <t:sankeywidget result="result" elementId="sankeychartcommonpartners"
                                                commonPartners="true"/>
                            </div>
                        </div>
                        <div class="grid-stack-item" data-gs-x="9" data-gs-y="0" data-gs-width="3" data-gs-height="4">
                            <div class="grid-stack-item-content">
                                <t:circlewidget result="result" elementId="circlechart"/>
                            </div>
                        </div>
                        <div class="grid-stack-item" data-gs-x="0" data-gs-y="5" data-gs-width="12" data-gs-height="6">
                            <div class="grid-stack-item-content">
                                <t:tableresultwidget result="result"/>
                            </div>
                        </div>
                    </div>
                </t:zone>
            </div>
        </div>
    </body>
</html>
