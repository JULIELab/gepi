<html t:type="layout" title="GePI Index"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd">
	<body class="${bodyScrollClass}">
		<div id="inputcol" class="center fade ${showInputClass}">
			<t:zone t:id="inputzone" id="inputzone">
				<t:gepiinput result="result" />
			</t:zone>
		</div>
		<div class="row">
			<div id="outputcol" class="col-md-12 fade ${showOutputclass}">
				<t:zone t:id="outputzone" id="outputzone">
					<div id="widgetOverlay" class="fade ${widgetOverlayShowClass}" />
					<div class="row">
						<t:piechartwidget result="result" class="col-md-4" />
						<t:sankeywidget result="result" class="col-md-4" />
						<t:sankeywidgetcommonpartners result="result"
							class="col-md-4" />
					</div>
					<div class="row">
						<t:barchartwidget result="result" class="col-md-12" />
					</div>
					<p>
						<t:actionlink t:id="download"> Download Table Results </t:actionlink>
					</p>
					<div class="row">
						<t:tableresultwidget result="result" class="col-md-12" />
					</div>
				</t:zone>
			</div>
		</div>
	</body>
</html>
